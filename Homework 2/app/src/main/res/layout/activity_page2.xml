<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Page2"
    android:background="@drawable/_c8b26d6e66d1e5e37f5e1ff6fd94e80">

    <TextView
        android:id="@+id/cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="130dp"
        android:text="Your CV"
        android:textColor="#315A33"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/nameOutput"
        android:layout_width="268dp"
        android:layout_height="40dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="70dp"
        android:ems="10"
        android:text="Name"
        android:textColor="#020202"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/ageOutput"
        android:layout_width="268dp"
        android:layout_height="43dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:ems="10"
        android:text="Age"
        android:textColor="#020202"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/jobOutput"
        android:layout_width="268dp"
        android:layout_height="41dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:ems="10"
        android:text="Your Job"
        android:textColor="#040303"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/phoneNumberOutput"
        android:layout_width="268dp"
        android:layout_height="43dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:ems="10"
        android:text="Phone Number"
        android:textColor="#020202"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/emailOutput"
        android:layout_width="269dp"
        android:layout_height="43dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:ems="10"
        android:text="Email"
        android:textColor="#040303"
        android:textSize="20sp" />


</LinearLayout>